  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon --watch server --exec babel-node -- server/dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean-win": "del build /s /q && mkdir build",
    "clean": "rm -r build && mkdir build",
    "build-server": "babel -d ./build ./server",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "start-s": "node --debug ./build/index.js",
    "build-client": "webpack --config webpack.config.prod.js && rsync -auv \"client/assets\" \"build/public\" && rsync -auv \"server/index.html\" \"build/public/index.html\"",
    "build-client-win": "webpack --config webpack.config.prod.js && xcopy \"client/assets\" \"build/public\" && xcopy \"server/index.html\" \"build/public/index.html\""
  },
